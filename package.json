{
  "name": "zashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "bash start-dev.sh",
    "dev:direct": "next dev --turbopack --port 3000",
    "dev:force": "bash start-dev.sh --force",
    "dev:check": "lsof -ti:3000 && echo 'Port 3000 is in use' || echo 'Port 3000 is available'",
    "port:kill": "lsof -ti:3000 | xargs kill -9 2>/dev/null || echo 'No processes found on port 3000'",
    "build": "next build --turbopack",
    "start": "next start --port 3000",
    "lint": "eslint",
    "db:init": "node scripts/init-db.js",
    "db:test": "node scripts/init-db.js --test"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.62.12",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.2",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.468.0",
    "next": "15.5.2",
    "next-auth": "^4.24.10",
    "pg": "^8.13.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "recharts": "^2.15.0",
    "tailwind-merge": "^3.3.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^3.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/pg": "^8.11.13",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
